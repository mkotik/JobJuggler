datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model users {
  id       Int      @id @default(autoincrement())
  name     String   @unique
  age      Int
  email    String   @unique
}

model invoices {
  id        Int      @id @default(autoincrement())
  recipient String   
  amount    Float  
  email     String   @unique
}
